<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="SAdCoreFrameworkSettingsPanelTemplate_v_1_1" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="Title" inherits="GameFontHighlightHuge" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="7" y="-22"/>
                    </Anchors>
                    <Color r="1" g="1" b="1"/>
                </FontString>
                <Texture parentKey="HorizontalLine">
                    <Size x="0" y="1"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.Title" relativePoint="BOTTOMLEFT" x="0" y="-8"/>
                        <Anchor point="TOPRIGHT" x="-30" y="-63"/>
                    </Anchors>
                    <Color r="0.25" g="0.25" b="0.25" a="1"/>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <ScrollFrame parentKey="ScrollFrame" inherits="UIPanelScrollFrameTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" x="0" y="-28"/>
                    <Anchor point="BOTTOMRIGHT" x="-30" y="10"/>
                </Anchors>
                <ScrollChild>
                    <Frame parentKey="Content">
                        <Size x="600" y="1"/>
                    </Frame>
                </ScrollChild>
            </ScrollFrame>
        </Frames>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsHeaderTemplate_v_1_1" virtual="true">
        <Size y="50"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Title" inherits="GameFontHighlightLarge" justifyH="LEFT" justifyV="BOTTOM">
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" x="7" y="4"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsCheckboxTemplate_v_1_1" virtual="true">
        <Size y="32"/>
        <Anchors>
            <Anchor point="LEFT"/>
            <Anchor point="RIGHT"/>
        </Anchors>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" wordwrap="false">
                    <Size x="205" y="0"/>
                    <Anchors>
                        <Anchor point="LEFT" x="17"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <CheckButton parentKey="CheckBox" motionScriptsWhileDisabled="true">
                <Size x="26" y="26"/>
                <Anchors>
                    <Anchor point="LEFT" x="215" y="0"/>
                </Anchors>
                <NormalTexture atlas="checkbox-minimal" useAtlasSize="true"/>
                <PushedTexture atlas="checkbox-minimal" useAtlasSize="true"/>
                <CheckedTexture atlas="checkmark-minimal" useAtlasSize="true"/>
                <DisabledCheckedTexture atlas="checkmark-minimal-disabled" useAtlasSize="true"/>
            </CheckButton>
        </Frames>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsDropdownTemplate_v_1_1" virtual="true">
        <Size y="32"/>
        <Anchors>
            <Anchor point="LEFT"/>
            <Anchor point="RIGHT"/>
        </Anchors>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" wordwrap="false">
                    <Size x="205" y="0"/>
                    <Anchors>
                        <Anchor point="LEFT" x="17"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="Dropdown" inherits="UIDropDownMenuTemplate">
                <Anchors>
                    <Anchor point="LEFT" x="200" y="3"/>
                </Anchors>
            </Frame>
        </Frames>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsSliderTemplate_v_1_1" virtual="true">
        <Size y="32"/>
        <Anchors>
            <Anchor point="LEFT"/>
            <Anchor point="RIGHT"/>
        </Anchors>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" wordwrap="false">
                    <Size x="205" y="0"/>
                    <Anchors>
                        <Anchor point="LEFT" x="17"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="Value" inherits="GameFontNormal" justifyH="LEFT">
                    <Anchors>
                        <Anchor point="LEFT" x="430" y="0"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Slider parentKey="Slider" inherits="MinimalSliderWithSteppersTemplate">
                <Size x="205" y="22"/>
                <Anchors>
                    <Anchor point="LEFT" x="215" y="0"/>
                </Anchors>
            </Slider>
        </Frames>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsButtonTemplate_v_1_1" virtual="true">
        <Size y="40"/>
        <Anchors>
            <Anchor point="LEFT"/>
            <Anchor point="RIGHT"/>
        </Anchors>
        <Frames>
            <Button parentKey="Button" inherits="UIPanelButtonTemplate">
                <Size x="120" y="22"/>
                <Anchors>
                    <Anchor point="LEFT" x="35" y="0"/>
                </Anchors>
            </Button>
        </Frames>
    </Frame>

    <Frame name="SAdCoreFrameworkSettingsInputButtonTemplate_v_1_1" virtual="true">
        <Size y="32"/>
        <Anchors>
            <Anchor point="LEFT"/>
            <Anchor point="RIGHT"/>
        </Anchors>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Text" inherits="GameFontNormal" justifyH="LEFT" wordwrap="false">
                    <Size x="205" y="0"/>
                    <Anchors>
                        <Anchor point="LEFT" x="17"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <EditBox parentKey="EditBox" autoFocus="false">
                <Size x="220" y="22"/>
                <Anchors>
                    <Anchor point="LEFT" x="218" y="0"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture parentKey="Background">
                            <Color r="0" g="0" b="0" a="0.5"/>
                        </Texture>
                    </Layer>
                </Layers>
                <FontString inherits="ChatFontNormal"/>
            </EditBox>
            <Button parentKey="Button" inherits="UIPanelButtonTemplate">
                <Size x="60" y="22"/>
                <Anchors>
                    <Anchor point="LEFT" relativeKey="$parent.EditBox" relativePoint="RIGHT" x="8" y="0"/>
                </Anchors>
            </Button>
        </Frames>
    </Frame>
</Ui>